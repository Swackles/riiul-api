{
  "name": "riiul-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "migrations": "node-pg-migrate -m ./dist/migrations up",
    "start": "node ./dist/app.js",
    "test": "jest",
    "migrate": "node-pg-migrate -m ./src/migrations -j ts"
  },
  "engines": {
    "node": "14.18.x",
    "npm": "7.17.x"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.1",
    "node-pg-migrate": "^6.0.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.5",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "jest": {
    "collectCoverage": true,
    "testRegex": "/__tests__/.*.tests.ts?$",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts",
      "!src/**/*.d.ts"
    ]
  }
}
